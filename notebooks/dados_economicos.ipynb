    "indicadores = {\"NY.GDP.MKTP.CD\": \"PIB total (US$ corrente)\",\n",
    "               \"NE.EXP.GNFS.ZS\": \"Exportações (% do PIB)\",\n",
    "               \"NE.IMP.GNFS.ZS\": \"Importações (% do PIB)\",\n",
    "               \"SP.POP.TOTL\": \"População total\",\n",
    "               \"NY.GDP.PCAP.CD\": \"PIB_per_capita\",\n",
